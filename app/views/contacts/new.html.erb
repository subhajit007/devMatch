<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="well">
      <%= form_for @contact do |f| %>
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :comments %>
          <%= f.text_area :comments, class: 'form-control' %>
        </div>
        <%= f.submit 'Submit', class: 'btn btn-default' %>


    </div>
  </div>
</div>

<div class="abc">

<div class="social center-block">
  <a href="https://www.facebook.com/devmatch" class="link facebook" target="_parent"><span class="fa fa-facebook-square"></span></a>
  <a href="https://github.com/subhajit007" class="link github" target="_parent"><span class="fa fa-github-square"></span></a>
  <a href="https://www.linkedin.com/pub/subhajit-banerjee/b1/465/1a8" class="link linkedin" target="_parent"><span class="fa fa-linkedin"></span></a>
        </div>
<% end %>
</div>
